<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-aFq/bzH65dt+w6FI2ooMVUpc+21e0SRygnTpmBvdBgSdnuTN7QbdgL+OapgHtvPp" crossorigin="anonymous">
  <title>Color Palette</title>
  <style>
    .color-blocks {
      display: flex;
      flex-wrap: wrap;
    }
    .color-block {
      width: 100px;
      height: 100px;
      text-align: center;
      line-height: 100px;
    }
  </style>
</head>
<body>
  <h1>Primary Colors:</h1>
  <div id="color-blocks" class="color-blocks"></div>

  <script>
    (function () {
      const colors = JSON.parse('<%- JSON.stringify(colors) %>');
      const colorBlocksContainer = document.getElementById('color-blocks');

      colors.forEach((color) => {
        const block = document.createElement('div');
        block.style.backgroundColor = color;
        block.innerText = color;
        block.classList.add('color-block');
        colorBlocksContainer.appendChild(block);
      });
    })();
  </script>
</body>
</html>